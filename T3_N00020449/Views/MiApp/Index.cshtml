@{ List<DetalleRutina> detalle = ((List<DetalleRutina>)Model); }

<body>
    <header>
        <div class="container">
            <h1><img src="https://simpleicon.com/wp-content/uploads/user1.png" alt="Alternate Text" width="80" /> @User.Claims.FirstOrDefault().Value     </h1>

            <hr />
            <br />
        </div>
    </header>
</body>

<label class="display-4">Lista de Ejercicios</label>

<table class="table table-dark">
    <thead>
        <tr class="bg-success">
            <th>Nombre de Ejercicio </th>
            <th>Link de Ejercicio </th>
            <th>Tiempo de Ejercicio </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in detalle)
        {
<tr>
    <td>@item.Ejercicios.Nombre</td>
    <td>@item.Ejercicios.LinkYoutube</td>
    <td>@item.Tiempo Segundos</td>
</tr>
}
    </tbody>
</table>